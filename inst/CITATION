citHeader("To cite the RAMClustR package in publications use:")

citEntry(entry="article",  
   author="C. D. Broeckling, F. A. Afsar, S. Neumann , A. Ben-Hur, and J. E. Prenni",
   title="RAMClust: A Novel Feature Clustering Method Enables Spectral-Matching-Based Annotation for Metabolomics Data",
   journal="Analytical Chemistry",
   year="2014",
   volume="86 (14)",
   pages="6812-6817",
   textVersion = paste("C. D. Broeckling, F. A. Afsar, S. Neumann , A. Ben-Hur, and J. E. Prenni:",
                       "RAMClust: A Novel Feature Clustering Method Enables Spectral-Matching-Based Annotation for Metabolomics Data",
                       "XCMS: Processing mass spectrometry data for metabolite profiling",
                       "using nonlinear peak alignment, matching and identification,",
                       "Analytical Chemistry, 2014, 86 (14), pp 6812-6817"))


citEntry(entry="article",  
   author="Broeckling, C. D.; Heuberger, A. L.; Prince, J. A.; Ingelsson, E.; Prenni, J. E.",
